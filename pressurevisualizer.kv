 # #:kivy

<RoundedButton@Button>:
    background_color: 0, 0, 0, 0
    background_normal: ''
    back_color: 0.3, 0.3, 0.3, 1
    size_hint: None, None
    size: 100, 100
    canvas.before:
        Color: 
            rgba: (0.3, 0.75, 0.86, 1) if self.state=='down' else self.back_color
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [1000]

<PressureVisualizer>:
    orientation: 'vertical'
    
    BoxLayout:
        orientation: 'horizontal'
        size_hint: (1.0, 0.1)

        # 背景色
        canvas.before:
            Color:
                rgb: 0.2, 0.2, 0.2
            Rectangle:
                size:self.size
                pos: self.pos

        # Pathの入力部分
        BoxLayout:

            orientation: 'vertical'
            size_hint: (0.9, 1.0)
            
            BoxLayout:
                orientation: 'horizontal'
                size_hint: (1.0, 0.4)
                spacing: 10

                AnchorLayout:
                    size_hint: (0.95, 1.0)
                    anchor_x: 'right'
                    anchor_y: 'bottom'

                    TextInput:
                        id: play_path
                        size_hint: .95, .7
                        text: root.default_path
                        font_size: self.height*0.5

                AnchorLayout:
                    size_hint: (0.05, 1.0)
                    anchor_x: 'left'
                    anchor_y: 'bottom'
                    Button:
                        size_hint: .8, .7
                        text: 'Set'
                        on_press: root.set_play_path()

            AnchorLayout:
                size_hint: (1.0, 0.25)
                anchor_x: 'right'
                anchor_y: 'center'
                
                Label:
                    size_hint: .95, .9
                    font_size: self.height*0.7
                    text_size: self.size
                    halign: 'right'
                    valign: 'center'
                    color: 0.6, 0.6, 0.6, 1.0
                    text: root.responce

            BoxLayout:
                orientation: 'horizontal'
                size_hint: (1.0, 0.35)
                spacing: 10

                AnchorLayout:
                    size_hint: (0.85, 1.0)
                    anchor_x: 'right'
                    anchor_y: 'center'

                    Slider:
                        id: sld
                        size_hint: .9, .3
                        min: 0
                        max: root.step
                        value: root.counter
                        on_touch_move: root.on_slider_move(self, self.value_normalized)

                Label:
                    size_hint: (0.15, 1.0)
                    font_size: self.height*0.5
                    text_size: self.size
                    halign: 'left'
                    valign: 'center'
                    color: 0.6, 0.6, 0.6, 1.0
                    text: 'Frame : ' + str(root.counter)


        # Playボタンの部分
        AnchorLayout:
            size_hint: (0.1, 1.0)
            anchor_x: 'center'
            anchor_y: 'center'        
            RoundedButton:
                id: playback_button
                size_hint: None, .7 # 縦の大きさだけ相対に
                size: self.height, 0    # 横の大きさを縦と同じに, 縦の大きさはhintが優先されるので無視
                text: 'Play'
                on_press: root.playback_start()

            MDSpinner:
                size_hint: None, .7
                size: self.height, 0
                active: True if root.playback_button_state else False

  
    
    BoxLayout:
        size_hint: (1.0, 0.9)
        orientation: 'horizontal'

        canvas.before:

            # 背景
            Color:
                rgb: 0.225, 0.225, 0.225
            Rectangle:
                size: self.size
                pos: self.pos

            Color:
                rgb: 0.1, 0.1, 0.1
            # 縦線
            Line:
                points: root.width*0.1, 0, root.width*0.1, self.height
            Line:
                points: root.width*0.2, 0, root.width*0.2, self.height
            Line:
                points: root.width*0.3, 0, root.width*0.3, self.height
            Line:
                points: root.width*0.4, 0, root.width*0.4, self.height
            Line:
                points: root.width*0.5, 0, root.width*0.5, self.height
