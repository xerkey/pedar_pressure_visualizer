                points: root.width*0.6, 0, root.width*0.6, self.height
            Line:
                points: root.width*0.7, 0, root.width*0.7, self.height
            Line:
                points: root.width*0.8, 0, root.width*0.8, self.height
            Line:
                points: root.width*0.9, 0, root.width*0.9, self.height

            # 横線
            Line:
                points: 0, self.height*0.1, root.width, self.height*0.1
            Line:
                points: 0, self.height*0.2, root.width, self.height*0.2
